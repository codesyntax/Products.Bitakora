<dtml-let capt="getCaptchaImage()">
<dtml-if "REQUEST.has_key('msg')">
<div class="system-msg"><dtml-var "REQUEST.get('msg')"></div>
</dtml-if>
<div id="bitakora_cpt_control">
<p><dtml-var "capt[1]"></p>
<p>
<dtml-var "gettext('Please enter the letters shown in the image in the following text box. Provided by Captchas.net')"> <br/>
<input type="text" name="bitakora_cpt" size="15" />
</p>
<input type="hidden" name="random_cpt" value="<dtml-var "capt[0]">" />
</div>
</dtml-let>